        <fullName>Email_Invoices_To_Users</fullName>
        <availability>online</availability>
        <description>This will email all generated invoices to the users.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Email Invoices To Users</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}


   // Now make a synchronous call to the Apex Web service
   // method
   var result = sforce.apex.execute(
    &quot;invoiceit_s/ClassWebServiceEmailing&quot;,  
    &quot;emailAllInvoicesForInvoiceRun&quot;, 
    {invoiceRunID: &quot;{!Invoice_Run__c.Id}&quot;});
   alert(result); //response</url>
    </webLinks>
