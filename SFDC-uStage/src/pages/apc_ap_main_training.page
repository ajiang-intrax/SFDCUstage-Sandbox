<apex:page docType="html-5.0" title="Education & Training" applyBodyTag="false" applyHtmlTag="false" standardController="Applicant_Info__c" extensions="AppEngine_Controller" action="{!checkLogin}" standardStylesheets="false" showHeader="false" id="apc_ap_main_training">
  <apex:composition template="intraxtemplate_vs3">
    <apex:define name="body">
      <c:block_header_apc_ap_application />
       <div id="app-page" class="row">
        <div id="app-focus" class="col-md-8 col-md-push-3">
          <c:block_app_completion ApplicantInfo="{!applicantInfo}" userProfileName="{!userProfileName}"  />
          
          <!-- Jose-Adding Tips for Partners  (Profile "OCPM Agent Portal")
          <apex:outputPanel rendered="{!userProfileName == 'OCPM Agent Portal' && applicantInfo.Application_Level__c == 'Main'}" layout="block" id="blockTip">
	          <div class="lead alert alert-warning clearfix">
	          <b>Tip</b>&nbsp;&nbsp;
	           Please check that the au pair's earliest program arrival date is after their date of graduation from Secondary School.
	          </div>
          </apex:outputPanel>
          -->
          
          <h1>Education and Training</h1>
          <p class="lead">All au pairs are required to graduate from secondary school before traveling as au pairs. Host families are also interested in any additional education, training or certifications you may have. Please answer all questions, select all skills that apply to you and upload at least one document as proof of completing secondary school or higher education. </p>
          <apex:form styleClass="form-vertical ws-validate" id="webform" >
            <apex:variable value="{!1}" var="rowNumf" id="rowCounterF"/>
            <apex:variable value="{!1}" var="rowNumcert" id="rowcert"/>
            <apex:Messages styleClass="custom-error-pagelevel" id="messageapex" />
              <div class="panel panel-default">
                <div class="panel-heading">
                  <h4 class="panel-title">Secondary School</h4>
                </div>
                <div class="panel-body">
                
                <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">School Name</label>
                    <apex:inputField styleClass="form-control" onblur="CapitalizeName(event.target);" value="{!educationInfo.Institution_Name__c}" required="TRUE" id="SchoolName" />
                  </div>
                  </div>
                   <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Graduation Date</label>
                    <div class="datepicker" locale="{!user.localeSidKey}">
                      <apex:inputField styleClass="form-control" html-placeholder="Graduation Date" value="{!personInfo.Graduation_Date__c}" required="TRUE" id="GraduationDate" />
                    </div>
                  </div>
                  </div>
                  </div><!--  end row -->
                  
                 <div class="row">
                <div class="col-sm-6"> 
                  <div class="form-group">
                    <label class="control-label">Address 1</label>
                    <apex:inputField html-placeholder="Street address" styleClass="form-control"  onblur="CapitalizeName(event.target);" value="{!educationInfo.Address_Street_1__c}" required="TRUE" id="Address" />
                  </div>
                  </div>
                  <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Address 2 <span class="info">(Optional)</span></label>
                    <apex:inputField html-placeholder="Apartment, building, floor, etc." styleClass="form-control"  onblur="CapitalizeName(event.target);" value="{!educationInfo.Address_Street_2__c}" required="False" id="Address2" />
                  </div>
                  </div>
                  </div><!--  end row -->
                  
                  
                  <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">City</label>
                    <apex:inputField styleClass="form-control"  onblur="CapitalizeName(event.target);" value="{!educationInfo.City__c}" required="TRUE" id="Schcity" />
                  </div>
                  </div>
                  <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">State/Province/Region <span class="optional">(Optional)</span></label>
                    <apex:inputField styleClass="form-control" onblur="CapitalizeName(event.target);" value="{!educationInfo.State__c}"  id="State" />
                  </div>
                  </div>
                  </div><!--  end row -->
                  
                  <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Postal Code</label>
                    <apex:inputField styleClass="form-control"  value="{!educationInfo.Postal_Code__c}" required="TRUE" id="Schpostal" />
                  </div>
                  </div>
                   <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Country</label>
                    <apex:inputField styleClass="form-control"  value="{!educationInfo.Country__c}" required="TRUE" id="Schcountry" />
                  </div>
                  </div>
                  </div><!--  end row -->
               
                  
                  <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Phone Country Code</label>
                    <apex:actionRegion >
                      <apex:selectList styleClass="form-control" 
                            value="{!educationInfo.Phone_Country_Code__c}" multiselect="false"
                            required="TRUE" id="countrycode" size="1">
                        <apex:selectOption itemValue="" itemLabel="Select Country Code" />
                        <apex:selectOptions value="{!listCountryCodeOptions}" />
                        <apex:actionSupport event="onchange" rerender="phonecode"
                                status="showcode" />
                      </apex:selectList>
                    </apex:actionRegion>
                    <apex:actionStatus onstart="disableSubmit();" onstop="enableSubmit();" startStyleClass="feedback-st" startText="Loading. Please wait..." id="showcode" />
                    <apex:message for="countrycode" styleClass="custom-error-radio" id="apexmess1" />
                  </div>
                  </div>
                  <div class="col-sm-6">
                  <div class="form-group">
                    <label class="control-label">Phone Number</label>
                    <apex:outputpanel id="phonecode" layout="block">
                      <div class="input-group"> <span class="input-group-addon">{!educationInfo.Phone_Country_Code__c}</span>
                        <apex:inputField styleClass="form-control onlynumber" value="{!educationInfo.Phone__c}" required="TRUE" id="phone" />
                      </div>
                    </apex:outputpanel>
                  </div>
                  </div>
                  </div><!--  end row -->
                  
                  <!-- Upload Start  -->
                  <apex:pageBlock id="Educational_Document"> 
                  <p>Please upload a diploma if it is available. You can also upload it later by revisiting this page. AuPairCare will need your diploma in order to match you.</p>
                    <div class="row">
                      <div class="col-sm-12 margin-t">
                        <apex:commandlink styleClass="btn btn-link btn-lg vs2 margin-r1" action="{!saveEducationapc}" immediate="false" reRender="webform" onComplete="popModal();rerunjs();" status="onsubmit">
                          <apex:param name="edu_id" value="{!educationInfo.id}"/>
                          <apex:param name="doctype2" value="Secondary-School-Diploma" />
                         <i class="fa fa-graduation-cap"></i> <span>Upload Diploma</span>
                        </apex:commandlink> 
                        
                      <i class="infobtn fa fa-question-circle" data-toggle="modal" data-target="#secdiploma"></i>
                      <div id="secdiploma" class="modal vs1 fade in">
                       <div class="modal-dialog modal-dialog-lg">
                       <div class="modal-content">
                       <a class="closebtn" data-dismiss="modal">
                            <span class="fa-stack fa-lg ">
                            <i class="fa fa-circle fa-stack-2x"></i>
                            <i class="fa fa-times fa-stack-1x fa-inverse"></i>
                            </span>
                        </a>
                       <div class="modal-body">
                    <div class="row">
                   <div class="col-lg-6"> 
                     <h3 class="inline-block">Sample Document</h3> <a target="_blank" href="https://s3-us-west-1.amazonaws.com/resources.intraxinc.com/all/documents/portals/apc/pt/app/graduation-date-certificate.jpg"><b>(See Full Size)</b></a>
                     <div class="margin-b4">
                     <img src="https://s3-us-west-1.amazonaws.com/resources.intraxinc.com/all/documents/portals/apc/pt/app/graduation-date-certificate.jpg"/></div>  
                       
                   </div>
                    <div class="col-lg-6"> 
                     <h3>Tips for Photographing Documents</h3>

<p>If you do not have access to a scanner you can use a digital camera or smartphone camera to capture the required documents. Please note that the copy must be clear with all text being readable. Below are a few tips for achieving this.</p>
  <ul>
<li>Do not use the flash when copying documents, as that will wash them out.</li>
<li>The best source of light is sunlight as it is bright enough to get a sharp, readable image.</li>
<li>Use a dark background for the image. You can place a piece of black or gray poster board or paper below the documents when you photograph them if you don't have dark surface.</li>
    <li>Keep the images square and centered in the photograph.</li>
</ul>
                   </div>
                     
                    </div>  <!--  end row -->
</div> <!--  end modal body -->
</div> <!--  end modal content -->
</div> <!--  end modal dialog -->
</div> <!-- end modal -->
                        
                        
                        
                        
                      </div>
                    </div>
                    <br/>
                    <!-- end form group --> 
                    <!--    </apex:outputPanel> -->
                    <apex:outputPanel layout="block" rendered="{!IF(DocListSchool.Size > 0, true, false)}">
                      <apex:pageBlock id="Block_Listschool">
                        <div class="table-responsive">
                          <apex:pageBlockTable styleClass="table vs2" value="{!DocmapEducation[educationInfo.id]}" var="item">
                            <apex:column headerValue="Document Name">
                              <apex:commandLink action="{!load}" rerender="" target="_blank">
                                <apex:param name="docGuid" value="{!item.Document_GUID__c}"/>
                                <apex:outputText value="{!item.Document_Name__c}"/>
                              </apex:commandLink>
                            </apex:column>
                            <apex:column headerValue="Remove" >
                              <div class="btn-toolbar no-margin">
                                <apex:commandLink action="{!iWantMyJSValues}" styleClass="btn-lg" oncomplete="confDelete();" immediate="true" rerender=""> <i class="fa fa-trash-o"></i>
                                  <apex:param name="docGuid" value="{!item.Document_GUID__c}" assignTo="{!docGuid}"/>
                                </apex:commandLink>
                              </div>
                            </apex:column>
                          </apex:pageBlockTable>
                        </div>
                        <!--  end table responsive --> 
                      </apex:pageBlock>
                    </apex:outputPanel>
                    <apex:outputText value="{!disableFileUploadText}" rendered="{!disableFileUpload}"/>
                  </apex:pageBlock>
                  <!--  end doc upload --> 
                  
                </div>
                <!--  end panel body --> 
              </div>
              <!--  end panel --> 
             <p>&nbsp;</p>
            <div class="section-title">Have you attended or are you attending a university or college? </div>
            
            <!--  start University Block -->
            
