                          <div class="col-sm-6">
                          <div class="form-group">
                            <label class="control-label">Country</label>
                            <apex:inputField styleClass="form-control" onblur="CapitalizeName(event.target);" Value="{!universityRecord.Country__c}"  required="TRUE" id="country" />
                          </div>
                          </div>
                          </div><!--  end row -->
                          
