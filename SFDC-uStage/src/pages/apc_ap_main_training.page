                          </div>
                          </div>
                          </div><!-- end row -->
                          
                          <div class="row">
                <div class="col-sm-6">
                          <div class="form-group">
                            <label class="control-label">City</label>
                            <apex:inputField styleClass="form-control" onblur="CapitalizeName(event.target);" Value="{!universityRecord.City__c}" required="TRUE" id="city" />
                          </div>
