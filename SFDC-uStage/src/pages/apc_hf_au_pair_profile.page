                    <b><apex:outputLink value="/apex/pr_pt_profile?participantId={!myProfile.participant.Id}">Return to Participant Summary</apex:outputLink></b>
                </apex:outputPanel>
                  </div>
                </div>
                <!--  end profile-nav --> 
                
              </div>
            </div>
            <!--  end row --> 
            
          </div>
          <!--  end col -->
          
          <div class="col-md-9">
            <div class="row">
              <div class="col-sm-4">
                <apex:outputpanel layout="block" id="blockCaseIstheOwner1" rendered="{!vUI = 'Becoming an au pair'}">
                  <apex:outputLink value="/apex/apc_ap_home"> <b class="sm">Return to Home Page</b> </apex:outputLink>
                </apex:outputpanel>
                <apex:outputpanel layout="block" id="blockNottheOwner" rendered="{!vUI == 'Hosting an au pair'}" >
                  <apex:outputLink value="/apex/apc_hf_search_au_pairs"> <b class="sm">Return to Search Results</b> </apex:outputLink>
                </apex:outputpanel>
                <apex:outputPanel layout="block" id="block_partner_back_to_search_2" rendered="{!NOT(vUI == 'Hosting an au pair' || vUI = 'Becoming an au pair')}">
                    <b><apex:outputLink value="/apex/pr_pt_profile?participantId={!myProfile.participant.Id}">Return to Participant Summary</apex:outputLink></b>
                </apex:outputPanel>
              </div>
              <div class="col-sm-8">
