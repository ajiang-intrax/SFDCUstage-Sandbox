                        <div class="form-group"> 
                            <div><label>First Available to Match</label></div>
                            <div><apex:outputfield value="{!myProfile.participant.Earliest_Match_Date__c}"/></div>
                        </div>
                    </div>   
                                     
                    <div class="col-sm-4">
                        <div class="form-group">    
                            <div><label>Last Available to Match</label></div>
                            <div><apex:outputfield value="{!myProfile.participant.Latest_Match_Date__c}"/></div>
                        </div>
                    </div>                                     
                 </apex:outputPanel>   
                   
                 <div class="col-sm-4">
                    <div class="form-group">    
                        <div>
                        
                        <label>
                            <apex:outputText value="Duration of Match" rendered="{!OR(AND(myProfile.participant.In_Country__c == 'Yes', NOT(ISNULL(myProfile.exitInterv))),AND(myProfile.participant.In_Country__c == 'Yes', myProfile.Applicant.Application_Type__c = 'Renewal'))}"/>
  							<apex:outputText value="Duration of Match" rendered="{!myProfile.participant.In_Country__c == 'No'}"/>
                        </label>
                            
                            
                            </div>
                            
                            
                        <div>
	                        <apex:outputText rendered="{!OR(AND(myProfile.participant.In_Country__c == 'Yes', NOT(ISNULL(myProfile.exitInterv))),AND(myProfile.participant.In_Country__c == 'Yes', myProfile.Applicant.Application_Type__c = 'Renewal'))}">
