                      <apex:commandLink status="matchactionupdate" styleClass="match-action-item" action="{!myProfile.saveAsFavorite}" id="saveAsFavorite" rerender="icons-block,icons-Menu" rendered="{!NOT(myProfile.isFavorite)}"> <i class="fa fa-heart" ></i> <span>Save</span> </apex:commandLink>
                      <apex:commandLink status="matchactionupdate" styleClass="match-action-item" action="{!myProfile.removeAsFavorite}" id="removeAsFavorite" rerender="icons-block,icons-Menu" rendered="{!myProfile.isFavorite}"> <i class="fa fa-heart selected" ></i> <span>Saved</span> </apex:commandLink>
                      <apex:commandLink status="matchactionupdate" styleClass="match-action-item" id="requestInterview" oncomplete="popModalInterview();" rendered="{!NOT(vIntProg)}"> <i class="fa fa-phone" ></i> <span>Interview</span> </apex:commandLink>
                      <apex:outputPanel styleClass="match-action-item" id="requestInterviewSelectedMenu" rendered="{!vIntProg}"> <i class="fa fa-phone selected" ></i> <span>Interview</span> </apex:outputPanel>
                      <apex:commandLink status="matchactionupdate" styleClass="match-action-item" action="{!myProfile.saveAsDismissed}" id="saveAsDismissed" rerender="icons-block,icons-Menu" rendered="{!NOT(myProfile.isDismissed)}"> <i class="fa fa-times-circle" ></i> <span>Remove</span> 
                        <!-- Rendering condition: rendered="{!hasInterviewInProgress} --> 
                        <!-- Rendering condition: rendered="{!NOT(hasInterviewInProgress)} --> 
                        <!-- Rendering condition: rendered="{!hadInterview} --> 
                      </apex:commandLink>
                      <apex:commandLink status="matchactionupdate" styleClass="match-action-item" action="{!myProfile.removeAsDismissed}" id="removeAsDismissed" rerender="icons-block,icons-Menu" rendered="{!myProfile.isDismissed}"> <i class="fa fa-times-circle selected" ></i> <span>Removed</span> </apex:commandLink>
                    </div>
                  </apex:outputPanel>
                </apex:pageBlock>
              </div>
            </div>
            <!--  end row -->
            
            <div id="over"></div>
            <div class="row">
