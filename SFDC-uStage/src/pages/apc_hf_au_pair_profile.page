                    <div class="form-group">
                        <div><label>Available On</label></div>
                        <div><apex:outputfield value="{!myProfile.participant.Earliest_Match_Date__c}"/></div>
                        </div>
                        </div>
                      </apex:outputPanel>
                      
                </apex:outputText>
                
                
                 <apex:outputPanel layout="block" rendered="{!AND(myProfile.participant.In_Country__c == 'Yes', myProfile.participant.Earliest_Match_Date__c != myProfile.participant.Latest_Match_Date__c, myProfile.participant.Latest_Match_Date__c != null)}">                        
