<apex:page controller="apc_portal_controller" action="{!myProfile.saveVisit}" docType="html-5.0" applyBodyTag="false" applyHtmlTag="false" standardStylesheets="false" showHeader="false" title="View Au Pairs" id="apc_hf_au_pair_profile">
