                      <li> <a onclick="goEducation();" href="#education" id="linkEducation"><span>Education and Training</span></a> </li>
                      <li> <a onclick="goDriving();" href="#driving" id="linkDriving"><span>Driving Experience</span></a> </li>
                      <li> <a onclick="goPersonal();" href="#family-members" id="linkPersonal"><span>Personal Life</span></a> </li>
                      <li> <a onclick="goHealth();" href="#health" id="linkHealth"><span>Overall Health</span></a> </li>
                      <apex:outputText rendered="{!myProfile.listChildCareRef.size > 0 || myProfile.listPersonalRef.size > 0}">
                        <li> <a onclick="goReference();" href="#reference" id="linkReference"><span>Reference Check</span></a> </li>
                      </apex:outputText>
                    </ul>
