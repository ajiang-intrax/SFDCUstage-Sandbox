            <apex:variable var="passport" value="{!IF(DocTypeList != NULL && DocTypeList['Passport'] == '1', 'complete', 'incomplete')}" />
            <apex:variable var="resume"  value="{!IF(DocTypeList != NULL && DocTypeList['Resume'] == '1', 'complete', 'incomplete')}" />
            <apex:variable var="joboffer"  value="{!IF(DocTypeList != NULL && DocTypeList['Offer-Document'] == '1', 'complete', 'incomplete')}" />
            <!-- <apex:variable var="tplanHC"  value="{!IF(DocTypeList != NULL && DocTypeList['DS-7002-(Company-Signature)'] == '1', 'complete', 'incomplete')}" />
            <apex:variable var="tplanPT"  value="{!IF(DocTypeList != NULL && DocTypeList['DS-7002-(Participant-Signature)'] == '1', 'complete', 'incomplete')}" />
            -->
            <apex:variable var="tplanComplete"  value="{!IF(DocTypeList != NULL && DocTypeList['DS-7002-(Complete)'] == '1', 'complete', 'incomplete')}" />
            <apex:variable var="financial"  value="{!IF(DocTypeList != NULL && DocTypeList['FinancialResponsibility'] == '1', 'complete', 'incomplete')}" />
            <apex:variable var="eligibility"  value="{!IF(DocTypeList != NULL && DocTypeList['Participant-Eligibility-Form'] == '1', 'complete', 'incomplete')}" />
