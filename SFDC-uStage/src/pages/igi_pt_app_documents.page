                    <apex:outputPanel styleClass="termsConditions margin-t collapse" layout="block" rendered="{!termsConditions == 'complete'}">
                      <apex:pageBlock id="Block_ListConditions">
                        <div class="table-responsive">
                          <table class="table">
                            <thead>
                              <tr>
                                <th>File Name</th>
                                <th>Remove</th>
                              </tr>
                            </thead>
                            <apex:repeat value="{!DocList}" var="item">
                              <apex:outputPanel rendered="{!IF(item.Document_Type__c == 'termsConditions', true, false)}">
                                <tr>
                                  <td><apex:commandLink action="{!load}" target="_blank" >
                                      <apex:param name="docGuid" value="{!item.Document_GUID__c}"/>
                                      <apex:outputText value="{!item.Document_Name__c}"/>
                                    </apex:commandLink></td>
                                  <td><apex:commandLink action="{!iWantMyJSValues}" styleClass="btn-lg" oncomplete="confDelete();" immediate="true" > <i class="fa fa-trash-o"></i>
                                      <apex:param name="docGuid" value="{!item.Document_GUID__c}" assignTo="{!docGuid}"/>
                                    </apex:commandLink></td>
                                </tr>
                              </apex:outputPanel>
                            </apex:repeat>
                          </table>
                        </div>
                        <!--  end table responsive --> 
                      </apex:pageBlock>
                    </apex:outputPanel>
                    <!--  table of uploads --> 
                  </div>
                  <!--  end docupload bttm --> 
                </div>
                <!--  end panel body --> 
              </apex:outputPanel>
              <!--  end panel --> 
              
            </div>
            <!--  end of upload list -->
            
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
              <div class="modal-dialog modal-sm">
                <div class="modal-content">
                  <div class="modal-body">
                    <p>Are you sure you want to remove this file?</p>
                  </div>
                  <div class="modal-footer">
                    <apex:commandLink styleClass="btn btn-primary" action="{!deletefile}" immediate="true">
                      <apex:param name="igiDocsParam" value="true" assignTo="{!igiDocsParam}" />
                      <apex:commandButton value="Yes" id="yesbutton"/>
                    </apex:commandLink>
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                  </div>
                </div>
              </div>
            </div>
            <!--  end modal -->
            
            <div class="modal fade modal-widget" id="myModalUpload" tabindex="-1" role="dialog" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h4 class="modal-title">Upload Document</h4>
                  </div>
                  <div class="modal-body">                     
                    <iframe width="100%" height="400" frameborder="0" scrolling="no" src="{!fileUploadUrl}pid={!ProgId}&aid={!$CurrentPage.parameters.Id}&orgid={!$Organization.Id}&docType={!$CurrentPage.parameters.docType2}&retparam={!retParam}"></iframe>
                  </div>
                </div>
              </div>
            </div>
            <!--  end modal --> 
            
            
              <div class="form-group margin-t4">
                <apex:outputText rendered="{!IF(applicantInfo.Application_Level__c == 'Main' && applicantInfo.Application_Stage__c == 'Working',  true, false)}">
                  <apex:commandlink styleClass="btn btn-primary btn-lg margin-r"  action="{!navigator}" id="nextlink2" onComplete="rerunjs();hwsize();" reRender="webform" status="onsubmit">
                    <apex:param name="nextPage" value="/apex/igi_pt_app_media" />
                    <apex:param name="reDirectPage" value="true" />
                    <apex:param name="docType" value="Participant-Photo" assignTo="{!docType}"/>
                    <apex:param name="pageId" value="061"/>
                    <apex:commandButton value="Save & Continue" id="continuebutton2"/>
                  </apex:commandlink>
                  <apex:commandlink styleClass="btn btn-link btn-lg" rendered="{!NOT(CONTAINS(applicantInfo.PageStatus_Complete__c,'061'))}"
                                    action="{!setPageIdStarted}" id="skipLink" reRender="webform" status="onsubmit">
                    <apex:param name="pageId" value="061"/>
                    <apex:commandButton value="" id="skipButton"/>
                    <span>Skip this page</span> </apex:commandlink>
                </apex:outputText>
              </div>
          
            
            
          </apex:form>
          
          <apex:form >
          	<c:block_app_coedit_authorization ApplicantInfo="{!applicantInfo}" pageController="{!AEC}" rendered="{!applicantInfo.Apply_on_Behalf_of_PT__c && applicantInfo.Application_Stage__c == 'Working'}"/>
          </apex:form>
         
        </div>
        <!-- end column  -->
         
          
        <div class="col-md-3 col-md-pull-7">
          <c:block_notification_sidebar ApplicantInfo="{!applicantInfo}" isPartner="{!isPartner}"/>
          <c:menu_igi_pt_app ApplicantInfo="{!applicantInfo}" personInfo="{!personInfo}" />
        </div>
        <!-- end column  --> 
      </div>
      <!-- end row --> 
