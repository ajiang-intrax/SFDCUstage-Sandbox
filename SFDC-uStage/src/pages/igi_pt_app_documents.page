var j$ = jQuery.noConflict();

function confDelete(){
j$('#myModal').modal();
} 
  
function popModal(){
var auxerr= j$('.custom-error-pagelevel').attr('role');
//alert(auxerr);
    if(auxerr=="alert"){
    }else{
        j$('#myModalUpload').modal();
    }
} 
    
</script> 
    </apex:define>
  </apex:composition>
</apex:page>