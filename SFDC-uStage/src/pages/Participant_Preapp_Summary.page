	                        <apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main' && ApplicantInfo.Application_Stage__c == 'Working' && NOT(isPartner)}" value="/apex/Participant_PreApp_Home?id={!applicantInfo.Id}"><span>Back</span></apex:outputLink>
