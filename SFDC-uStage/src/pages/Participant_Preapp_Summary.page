                        <div class="data-row">
                            <div class="data-label left"> First Name </div>
                            <div class="data-field">
                                <apex:outputField id="First_Name" value="{!personInfo.First_Name__c}" />
                            </div>
                        </div>
                        
                        <!-- First Name -->
                        <div class="data-row">
                            <div class="data-label left"> Middle Name </div>
                            <div class="data-field">
                                <apex:outputField id="Middle_Name" value="{!personInfo.Middle_Name__c}" />
                            </div>
                        </div>
                           
                        <!-- Middle Name -->
                        <div class="data-row">
                            <div class="data-label left"> Last Name </div>
                            <div class="data-field">
                                <apex:outputField id="Last_Name" value="{!personInfo.Last_Name__c}" />
                            </div>
                        </div>
                
                
                      <!-- Gender -->  
                        <div class="data-row">
                            <div class="data-label left"> Gender </div>
                            <div class="data-field">
                                <apex:outputField id="Gender" value="{!personInfo.Gender__c}" />
                            </div> 
                        </div>
                 
                       <!-- Birth Date -->  
                        <div class="data-row">
                            <div class="data-label left">Birth Date</div>
                            <div class="data-field">
                                <apex:outputField id="Date_of_Birth" value="{!personInfo.Date_of_Birth__c}"/>
                            </div>
                        </div>
                        
                        <!-- Country Of Citizenship -->
                        <div class="data-row">
                            <div class="data-label left">Country of Citizenship</div>
                            <div class="data-field">
                                <apex:outputField id="Citizenship" value="{!personInfo.Citizenship__c}"/>
                            </div>
                        </div>
                        
                                                
                        <!-- City of Birth -->
                        <div class="data-row">
                            <div class="data-label left">City of Birth</div>
                            <div class="data-field">
                                <apex:outputField id="Birth_City" value="{!personInfo.Birth_City__c}"/>
                            </div>
                        </div>
                        
                        <!-- Country of Birth -->
                        <div class="data-row">
                            <div class="data-label left">Country of Birth</div>
                            <div class="data-field">
                                <apex:outputField id="Birth_Country" value="{!personInfo.Birth_Country__c}"/>
                            </div>
                        </div>
                        
                         <!-- Country of Legal Residence -->
                        <div class="data-row">
                            <div class="data-label left">Country of Legal Residence</div>
                            <div class="data-field">
                                <apex:outputField id="Country_of_Residence" value="{!personInfo.Country_of_Residence__c}"/>
                            </div>
                        </div>
                        </div>  
                        
                                            
                    
                       <!-- Page Panel -->
                       <div>
                       
                       <h3 class="data-block-title">Biographical Documents</h3> 
                       
                       <apex:outputPanel styleClass="form-row" rendered="{!NOT(DocListAll.Size > 0)}">
                           <apex:outputLabel id="no-records" > <span class="no-records">No record(s) to display.</span><br/></apex:outputLabel>
                       </apex:outputPanel>
    
                       <!-- Table having details of documents -->
                       <apex:pageBlock id="Block_DocList" rendered="{!if(DocListAll.Size > 0, true, false)}">
                           <apex:pageBlockTable id="Table_DocList" styleClass="pageblocktable" value="{!DocListAll}" var="item">
                               
                               <!-- File Name -->
                               <apex:column headerValue="File Name">
                                   <apex:outputText id="DocName" value="{!item.Document_Name__c}"/>
                               </apex:column>
                               
                               <!-- Description -->
                               <apex:column headerValue="Description">
                                   <apex:outputText id="DocDescription" value="{!item.Description__c}"/>
                               </apex:column>
                               
                               <!-- Document GUID -->
                               <apex:column >
                                   
                                   <apex:commandlink id="LoadDoc" styleClass="inline-link" action="{!load}" target="_blank">
                                       <apex:param name="docGuid" value="{!item.Document_GUID__c}"/>
                                       <apex:commandButton value="View" />
                                   </apex:commandlink>
                               </apex:column>
                               
                           </apex:pageBlockTable>
                       </apex:pageBlock>         
                     
                         <!--  Heading of Panel -->
                       <h3 class="data-block-title">Contact Details</h3> 
                     
                        <!-- Address 1 -->
                        <div class="data-row">
                            <div class="data-label left"> Address 1 </div>
                            <div class="data-field">
                                <apex:outputField id="Home_Street_1" value="{!applicantInfo.Home_Street_1__c}" />
                            </div>
                        </div>
                           
                       <!-- Address 2 -->
                        <div class="data-row">
                            <div class="data-label left"> Address 2 </div>
                            <div class="data-field">
                                <apex:outputField id="Home_Street_2" value="{!applicantInfo.Home_Street_2__c}" />
                            </div>
                        </div>
                
                        <!-- City -->
                        <div class="data-row">
                            <div class="data-label left"> City </div>
                            <div class="data-field">
                                <apex:outputField id="Home_City" value="{!applicantInfo.Home_City__c}" />
                            </div>
                        </div>
                
                      <!-- State/Provience -->  
                        <div class="data-row">
                            <div class="data-label left"> State/Provience </div>
                            <div class="data-field">
                                <apex:outputField id="Home_State" value="{!applicantInfo.Home_State__c}" />
                            </div> 
                        </div>
                 
                        <!-- Postal Code -->  
                        <div class="data-row">
                            <div class="data-label left">Postal Code</div>
                            <div class="data-field">
                                <apex:outputField id="Home_Postal_Code" value="{!applicantInfo.Home_Postal_Code__c}"/>
                            </div>
                        </div>
                        
                        <!-- Country -->
                        <div class="data-row">
                            <div class="data-label left">Country</div>
                            <div class="data-field">
                                <apex:outputField id="Home_Country" value="{!applicantInfo.Home_Country__c}"/>
                            </div>
                        </div>
                        
                        <!-- Time Zone-->
                        <div class="data-row">
                            <div class="data-label left">Time Zone</div>
                            <div class="data-field">
                                <apex:outputField id="TimeZoneSidKey" value="{!user.TimeZoneSidKey}"/>
                            </div>
                        </div>
                        
                                                
                       <!-- Phone -->
                        <div class="data-row">
                            <div class="data-label left">Phone</div>
                            <div class="data-field">
                                <apex:outputField id="Phone" value="{!personInfo.Phone__c}"/>
                            </div>
                        </div>
                        
                       <!-- Mobile Phone -->
                        <div class="data-row">
                            <div class="data-label left">Mobile Phone</div>
                            <div class="data-field">
                                <apex:outputField id="Mobile" value="{!personInfo.Mobile__c}"/>
                            </div>
                        </div>
                        
                         <!-- Email Address -->
                        <div class="data-row">
                            <div class="data-label left">Email Address</div>
                            <div class="data-field">
                                <apex:outputField id="Email" value="{!personInfo.Email__c}"/>
                            </div>
                        </div>
                        
                        <!-- Skype Id -->
                        <div class="data-row">
                            <div class="data-label left">Skype Id</div>
                            <div class="data-field">
                                <apex:outputField id="Skype_Id" value="{!personInfo.Skype_Id__c}"/>
                            </div>
                        </div>
                   </div>
                   
                   <!-- Page Panel -->
                       <div>     
                     
                         <!--  Heading of Panel -->
                       <h3 class="data-block-title">Emergency Contacts</h3> 
                       
                    <apex:outputLabel rendered="{!IF(EmergencyInfoList.Size > 0, false, true)}"><span class="no-records">No emergency contacts indicated.</span><br/></apex:outputLabel>
                    <apex:pageBlock id="Block_EmergencyInfoList" rendered="{!IF(EmergencyInfoList.Size > 0, true, false)}" >
                         <apex:pageBlockTable id="Table_EmergencyInfoList" styleClass="pageblocktable" value="{!EmergencyInfoList}" var="item">
                                
                               <apex:column value="{!item.First_Name__c} {!item.Last_Name__c}" headerValue="Name" /> 
                               <apex:column value="{!item.Role_Other__c}" headerValue="Relationship"  />
                               <apex:column value="{!item.Mobile__c}" headerValue="Phone" /> 
                               <apex:column value="{!item.Email__c}" headerValue="Email"/>
                         </apex:pageBlockTable>  
                    </apex:pageBlock>
                       
                    </div>
                 
                   <!-- Page Panel -->
                     <div>
                           
                       <!--  Heading of Panel -->
                       <h3 class="data-block-title">Education Details</h3>
                    
                      <!-- Current Educational Status -->  
                        <apex:outputPanel rendered="{! IF(applicantInfo.Intrax_Program__c == 'Internship', true, false)}">
                          <div class="data-row">
                            <div class="data-label left"> Current Educational Status </div>
                            <div class="data-field">
                                <apex:outputField id="Primary_Activity" value="{!applicantInfo.Primary_Activity__c}" />
                            </div>
                          </div> 
                        </apex:outputPanel>
                 
                       <!-- College or University Name -->
                        <div class="data-row">
                            <div class="data-label left">College or University Name</div>
                            <div class="data-field">
                                <apex:outputField id="School_Name" value="{!applicantInfo.School_Name__c}"/>
                            </div>
                        </div>
                        
                         <!-- Field of Study -->
                        <div class="data-row">
                            <div class="data-label left">Field of Study</div>
                            <div class="data-field">
                                <apex:outputField id="Major" value="{!applicantInfo.Major__c}"/>
                            </div>
                        </div>
                        
                                                
                       <!-- Approximate Graduation Date -->
                        <apex:outputPanel rendered="{! IF(applicantInfo.Intrax_Program__c == 'Internship', true, false)}">
                          <div class="data-row">
                            <div class="data-label left">Approximate Graduation Date</div>
                            <div class="data-field">
                                <apex:outputField id="Graduation_Date" value="{!applicantInfo.Graduation_Date__c}"/>
                            </div>
                          </div>
                        </apex:outputPanel>
                        
                      </div>
                                             
                                           
                     <!--  Heading of Panel -->                       
                       
