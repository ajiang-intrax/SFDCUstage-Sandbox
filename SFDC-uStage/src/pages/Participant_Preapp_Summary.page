                       <!-- 
                           <p>Please review your answers carefully before you submit your Basic Application. 
                               Click <strong>View PDF Summary</strong> to print or save your information for your records.</p>
                           <p>Once you have reviewed all of your information, click <strong>Submit Application</strong> to 
                               complete the Basic Application.</p>
                       -->
                       </div>
              
                       <!-- Page Panel -->
                       <apex:outputPanel rendered="{!IF(applicantInfo.Intrax_Program__c == 'Internship', true, false)}">
          
                    <!-- Panel Heading -->
                        <h3 class="data-block-title">Program Selection</h3> 
                        
                        <!-- What Program are you applying for? -->
                        <div class="data-row">
                            <div class="data-label left"> What program are you applying for? </div>
                            <div class="data-field">
                                <apex:outputField id="Intrax_Program_Options" value="{!applicantInfo.Intrax_Program_Options__c}" />
                            </div>
                        </div>
                     </apex:outputPanel>
                        
                        <!-- Page Panel -->
                       <div>
          				
          				<!--  <apex:outputPanel rendered="{! IF(applicantInfo.Intrax_Program__c == 'Work Travel', true, false)}"> -->
          				<apex:outputPanel rendered="{!applicantInfo.Intrax_Program__c == 'Work Travel' && PhotoListAll.Size == 0 && (ApplicantInfo.Application_Stage__c == 'Accepted' || ApplicantInfo.Application_Stage__c == 'In-Review' || ApplicantInfo.Application_Stage__c == 'Submitted')}">
          					<h3 class="data-block-title">Upload Your Photo</h3> 
          					<div class="data-row">
	          					<div class="form-button" >
	                         		<apex:outputLink value="/apex/Participant_Photo_Upload?docType=WT-Participant-Photo&id={!applicantInfo.Id}&view=1"><span align="center">Upload Photo</span></apex:outputLink>
	                       		</div>
	                       	</div>
          				</apex:outputPanel>
                    <!-- Panel Heading -->
                        <h3 class="data-block-title">Program Preferences</h3> 
                        
                        <!-- Other Company -->
                        <apex:outputPanel rendered="{! applicantInfo.Intrax_Program_Options__c == 'Business Internship' || applicantInfo.Intrax_Program_Options__c == 'Hospitality Internship' }">
                        <div class="data-row">
                            <div class="data-label left">Have you already secured an internship offer from a company?</div>
                            <div class="data-field">
                                <apex:outputText id="ServiceLevelYes" value="Yes" rendered="{!applicantInfo.Service_Level__c == 'Independent'}" />
                                <apex:outputText id="ServiceLevelNo" value="No" rendered="{!applicantInfo.Service_Level__c == 'Premium'}" />
                            </div>
                        </div>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{! IF(applicantInfo.Intrax_Program__c == 'Work Travel', true, false)}">
                        <div class="data-row">
                            <!--<div class="data-label left">Have you already secured an offer from a company?</div>
                            <div class="data-field">
                                <apex:outputText id="ServiceLevelYesWT" value="Yes" rendered="{!applicantInfo.Service_Level__c == 'Independent'}" />
                                <apex:outputText id="ServiceLevelNoWT" value="No" rendered="{!applicantInfo.Service_Level__c == 'Premium'}" />
                            </div> -->
                            <div class="data-label left">How will you secure a position?</div>
                            <div class="data-field">
                                <apex:outputText id="ServiceLevelYesWT" value="I have already secured my own position independently of Intrax (“Intrax Independent”)" rendered="{!applicantInfo.Service_Level__c == 'Independent'}" />
                                <apex:outputText id="ServiceLevelNoWT" value="I plan to secure a position through an Intrax Hiring Event, in person or via Skype (“Intrax Premium”)" rendered="{!applicantInfo.Service_Level__c == 'Premium'}" />
                            </div> 
                        </div>
                        </apex:outputPanel>
                        
                        <!-- Engagement Start -->
                        <div class="data-row">
                            <div class="data-label left"> Earliest Start Date </div>
                            <div class="data-field">
                                <apex:outputField id="Engagement_Start" value="{!applicantInfo.Engagement_Start__c}" />
                            </div>
                        </div>
                
                        <!-- Engagement End -->
                        <div class="data-row">
                            <div class="data-label left"> Latest End Date </div>
                            <div class="data-field">
                                <apex:outputField id="Engagement_End" value="{!applicantInfo.Engagement_End__c}" />
                            </div>
                        </div>
                
                      <!-- Requested Duration -->  
                        <apex:outputPanel styleClass="data-row" rendered="{!IF(applicantInfo.Intrax_Program__c == 'Internship', true, false)}">
