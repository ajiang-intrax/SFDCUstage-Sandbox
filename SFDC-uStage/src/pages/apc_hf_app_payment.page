                                                    <label class="control-label">Country</label>
                                                        <apex:inputField styleClass="form-control" onblur="CapitalizeName(event.target);" value="{!payment.Billing_Country__c}"
                                                            required="TRUE" id="country" />
                                                            <apex:message for="country" styleClass="custom-error-text" />
                                                </div>
                                                </div>
                                                <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">
                                                        Postal Code
                                                    </label>
                                                        <apex:inputField styleClass="form-control" value="{!payment.Billing_Postal_Code__c}"
