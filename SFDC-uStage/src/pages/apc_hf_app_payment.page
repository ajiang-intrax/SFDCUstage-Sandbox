    <apex:composition template="intraxtemplate_vs3">
        <apex:define name="body">    
        <c:block_header_apc_hf_application userAgentCheck="{!userAgentCheck}"/> 
                 <div id="app-page" class="row">
        <div id="app-focus" class="col-md-8 col-md-push-3">
                    <apex:form id="paymentcontainer" styleClass="form-vertical ws-validate">
                     
                    <apex:pageBlock id="add-promocode">
                    <apex:outputPanel layout="block" id="paymentfirst">
