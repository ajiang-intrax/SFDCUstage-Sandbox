                                            <apex:inputField value="{!applicantinfo.Extension_Payment_Agreed__c}" required="FALSE" id="paymentagreed" />&nbsp;
                                            <span>I understand that upon application approval I am responsible for extension program fees in the amount of ${!totalamount}.
                                            I have indicated that I would like to pay these fees (insert choices from preference page)</span>
                                            </div>
                                            
                                        </apex:outputText>
                                    
                                    </apex:outputPanel> <!--  end BLOCK "paymentamount" -->
            
                                    
                                    <apex:outputPanel id="paymentfields" styleClass="paymentfields" layout="block">
                                        
                                    
