                                        <apex:outputText rendered="{!promovalue == 'freePromo'}">
                                            <div class="margin-t3">
                                            <h3>Good news! You do not have to pay an application fee.</h3>
                                                <apex:commandlink styleClass="btn btn-primary btn-lg" action="{!navigator}" id="submitlink" immediate="true">
                                                    <apex:param name="nextPage" value="/apex//apex/apc_hf_app_home" />
                                                    <apex:param name="LevelAppUp" value="true" />
                                                    <apex:param name="reDirectPage" value="true" />
                                                    <apex:param name="viewpage" value="true" />
                                                    <apex:param name="vstate" value="newprf" />
