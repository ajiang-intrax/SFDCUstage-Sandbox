                                            </div><!--  end row -->
                                            </div>
                                            </div>
                                            
                                            </div><!--  end row -->

                                            
                                            

                                            <apex:outputPanel layout="block" id="addressfieldsPanel">
                                            <p>&nbsp;</p>
                                            <div class="section-title">Billing Address</div>
                                                
                                                <div class="row">
                                                <div class="col-sm-12 margin-b">            
