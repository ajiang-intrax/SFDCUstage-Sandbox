	                      <div class="data-row">
	                          <div class="data-label left">Country of Birth</div>
	                          <div class="data-field">
	                              <apex:outputField id="Birth_Country" value="{!personInfo.Birth_Country__c}"/>
	                          </div>
	                      </div>
	                      
	                       <!-- Country of Legal Residence -->
	                      <div class="data-row">
	                          <div class="data-label left">Country of Legal Residence</div>
	                          <div class="data-field">
	                              <apex:outputField id="Country_of_Residence" value="{!personInfo.Country_of_Residence__c}"/>
	                          </div>
	                      </div>
	                      </div>  
	                      
	                                          
                    
                       <!-- Page Panel -->
                       <div>     
                 		
                 	    	<!--  Heading of Panel -->
                 			<h3 class="data-block-title">Contact Details</h3> 
               			
               				 <!-- Address 1 -->
	                      <div class="data-row">
	                          <div class="data-label left"> Address 1 </div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_Street_1" value="{!applicantInfo.Home_Street_1__c}" />
	                          </div>
	                      </div>
	                         
	                     <!-- Address 2 -->
	                      <div class="data-row">
	                          <div class="data-label left"> Address 2 </div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_Street_2" value="{!applicantInfo.Home_Street_2__c}" />
	                          </div>
	                      </div>
	              
	                      <!-- City -->
	                      <div class="data-row">
	                          <div class="data-label left"> City </div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_City" value="{!applicantInfo.Home_City__c}" />
	                          </div>
	                      </div>
	              
	              		  <!-- State/Provience -->	
	                      <div class="data-row">
	                          <div class="data-label left"> State/Provience </div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_State" value="{!applicantInfo.Home_State__c}" />
	                          </div> 
	                      </div>
	               
	               		   <!-- Postal Code -->	
	                      <div class="data-row">
	                          <div class="data-label left">Postal Code</div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_Postal_Code" value="{!applicantInfo.Home_Postal_Code__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Country -->
	                      <div class="data-row">
	                          <div class="data-label left">Country</div>
	                          <div class="data-field">
	                              <apex:outputField id="Home_Country" value="{!applicantInfo.Home_Country__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Time Zone-->
	                      <div class="data-row">
	                          <div class="data-label left">Time Zone</div>
	                          <div class="data-field">
	                              <apex:outputField id="TimeZoneSidKey" value="{!user.TimeZoneSidKey}"/>
	                          </div>
	                      </div>
	                      
	                      	                      
	                     <!-- Phone -->
	                      <div class="data-row">
	                          <div class="data-label left">Phone</div>
	                          <div class="data-field">
	                              <apex:outputField id="Phone" value="{!personInfo.Phone__c}"/>
	                          </div>
	                      </div>
	                      
	                     <!-- Mobile Phone -->
	                      <div class="data-row">
	                          <div class="data-label left">Mobile Phone</div>
	                          <div class="data-field">
	                              <apex:outputField id="Mobile" value="{!personInfo.Mobile__c}"/>
	                          </div>
	                      </div>
	                      
	                       <!-- Email Address -->
	                      <div class="data-row">
	                          <div class="data-label left">Email Address</div>
	                          <div class="data-field">
	                              <apex:outputField id="Email" value="{!personInfo.Email__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Skype Id -->
	                      <div class="data-row">
	                          <div class="data-label left">Skype Id</div>
	                          <div class="data-field">
	                              <apex:outputField id="Skype_Id" value="{!personInfo.Skype_Id__c}"/>
	                          </div>
	                      </div>
             			</div>
             			
             			<!-- Page Panel -->
                       <div>     
                 		
                 	    	<!--  Heading of Panel -->
                 			<h3 class="data-block-title">Emergency Contacts</h3> 
                 			
				            <apex:outputLabel rendered="{!IF(EmergencyInfoList.Size > 0, false, true)}"><span class="no-records">No emergency contacts indicated.</span><br/></apex:outputLabel>
				          <apex:repeat value="{!EmergencyInfoList}" var="item" >
						      <div class="data-row">
						        <div class="data-label left">Emergency Contact Name</div>
						        <div class="data-field">
						          <apex:outputText id="Full_Name" value="{!item.First_Name__c} {!item.Middle_Name__c} {!item.Last_Name__c}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">Relationship</div>
						        <div class="form-field">
						          <apex:outputText id="Role" value="{!item.Role__c}" rendered="{! IF(NOT(item.Role__c == 'Other'), true, false)}"/>
						          <apex:outputfield id="Role_Other" value="{!item.Role_Other__c}" rendered="{! IF(NOT(item.Role__c == 'Other'), false, true)}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">Email</div>
						        <div class="form-field">
						          <apex:outputText id="Item_Email" value="{!item.Email__c}"/>
						        </div>
						      </div>
					    </apex:repeat>
               				
             		   </div>
             		
             			<!-- Page Panel -->
             			  <div>
             			      	
                 			<!--  Heading of Panel -->
                 			<h3 class="data-block-title">Education Details</h3>
              			
	              			<!-- Current Educational Status -->	
	                      <div class="data-row">
	                          <div class="data-label left"> Current Educational Status </div>
	                          <div class="data-field">
	                              <apex:outputField id="Primary_Activity" value="{!applicantInfo.Primary_Activity__c}" />
	                          </div> 
	                      </div>
	               
	               		  <!-- College or University Name -->
	                      <div class="data-row">
	                          <div class="data-label left">College or University Name</div>
	                          <div class="data-field">
	                              <apex:outputField id="School_Name" value="{!applicantInfo.School_Name__c}"/>
	                          </div>
	                      </div>
	                      
	                       <!-- Field of Study -->
	                      <div class="data-row">
	                          <div class="data-label left">Field of Study</div>
	                          <div class="data-field">
	                              <apex:outputField id="Major" value="{!applicantInfo.Major__c}"/>
	                          </div>
	                      </div>
	                      
	                      	                      
	                     <!-- Approximate Graduation Date -->
	                      <div class="data-row">
	                          <div class="data-label left">Approximate Graduation Date</div>
	                          <div class="data-field">
	                              <apex:outputField id="Graduation_Date" value="{!applicantInfo.Graduation_Date__c}"/>
	                          </div>
	                      </div>
	                      </div>
	                     	              			
                    	                 		
                 		<!--  Heading of Panel -->                 			
                 			
                 		<div >  
                 			                			
                 			<h3 class="data-block-title">Language Skills</h3>              			
	              			              			
               				<!-- Primary Language -->
	                      <div class="data-row">
	                          <div class="data-label left"> Primary Language </div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_1" value="{!applicantInfo.Language_1__c}" />
	                          </div>
	                      </div>
	                         
	                     <!-- Primary language Level -->
	                      <div class="data-row">
	                          <div class="data-label left"> Primary language Level </div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_1_Proficiency" value="{!applicantInfo.Language_1_Proficiency__c}" />
	                          </div>
	                      </div>
	              
	                      <!-- Secondary language -->
	                      <div class="data-row">
	                          <div class="data-label left"> Secondary language </div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_2" value="{!applicantInfo.Language_2__c}" />
	                          </div>
	                      </div>
	              
	              		  <!-- Secondary language level -->	
	                      <div class="data-row">
	                          <div class="data-label left"> Secondary language level </div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_2_Proficiency" value="{!applicantInfo.Language_2_Proficiency__c}" />
	                          </div> 
	                      </div>
	               
	               		   <!-- Tertiary language -->	
	                      <div class="data-row">
	                          <div class="data-label left">Tertiary language</div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_3" value="{!applicantInfo.Language_3__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Tertiary language level -->
	                      <div class="data-row">
	                          <div class="data-label left">Tertiary language level</div>
	                          <div class="data-field">
	                              <apex:outputField id="Language_3_Proficiency" value="{!applicantInfo.Language_3_Proficiency__c}"/>
	                          </div>
	                      </div>
	                      
	                      	                      
	                     <!-- Other languages -->
	                      <div class="data-row">
	                          <div class="data-label left">Other languages</div>
	                          <div class="data-field">
	                              <apex:outputField id="Other_Languages" value="{!applicantInfo.Other_Languages__c}"/>
	                          </div>
	                      </div>
	                   
	                   
	                   <div>     
                 		
                 	    	<!--  Heading of Panel -->
                 			<h3 class="data-block-title">Experience Details</h3> 
                 			
				            <apex:outputLabel rendered="{!IF(EmergencyInfoList.Size > 0, false, true)}"><span class="no-records">No Experience indicated.</span></apex:outputLabel>
				          <apex:repeat value="{!listExperience}" var="item" >
						      <div class="data-row">
						        <div class="data-label left">Employer Name</div>
						        <div class="data-field">
						          <apex:outputText id="Employer_Name" value="{!item.Employer_Name__c}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">City</div>
						        <div class="form-field">
						          <apex:outputText id="City" value="{!item.City__c}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">Country</div>
						        <div class="form-field">
						          <apex:outputText id="Country" value="{!item.Country__c}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">Start Date</div>
						        <div class="form-field">
						          <apex:outputField id="Start_Date" value="{!item.Start_Date__c}"/>
						        </div>
						      </div>
						      
						      <div class="data-row">
						        <div class="data-label left">End Date</div>
						        <div class="form-field">
						          <apex:outputField id="End_Date" value="{!item.End_Date__c}"/>
