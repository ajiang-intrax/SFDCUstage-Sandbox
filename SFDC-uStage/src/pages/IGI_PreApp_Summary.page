<apex:page id="IGI_PreApp_Summary" standardController="Applicant_Info__c" extensions="AppEngine_Controller" action="{!checkLogin}"  standardStylesheets="true" showHeader="false" >    
	<apex:composition template="IntraxTemplate_Vs1">
        <apex:define name="body">
            <div class="wrapper" id="IGI_PreApp_Summary">
                <apex:form id="form" >
                
                    <!-- Page Title -->
                    <div class="page-header clearfix">
                        <h1 class="page-title">Intrax Global Internship Application</h1>
                        <div class="back-to-main">
                        	<apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Basic' && ApplicantInfo.Application_Stage__c == 'Working' && NOT(isPartner)}" value="apex/IGI_PreApp_Home?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
                        	<apex:outputLink styleClass="no-padding" rendered="{! NOT(Applicant_Info__c.Application_Level__c == 'Basic' && ApplicantInfo.Application_Stage__c == 'Working') && NOT(isPartner)}" value="apex/your_applications"><span>Back to Main Page</span></apex:outputLink>
                        	<apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Basic' && ApplicantInfo.Application_Stage__c == 'Working' && isPartner}" value="apex/IGI_PreApp_Home?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
                        	<apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main' && ApplicantInfo.Application_Stage__c == 'Working' && isPartner}" value="/apex/IGI_PreApp_Home?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
		                    <apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main' && $CurrentPage.parameters.BackURL == 'PT'  && NOT(ApplicantInfo.Application_Stage__c == 'Working') && isPartner}" value="/apex/pr_pt_profile?participantId={!participantId}"><span>Back to Main Page</span></apex:outputLink>
		                    <apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main' && $CurrentPage.parameters.BackURL == 'AP'  && NOT(ApplicantInfo.Application_Stage__c == 'Working') && isPartner}" value="/apex/pr_app_list"><span>Back to Main Page</span></apex:outputLink>
		                    <apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main' && NOT($CurrentPage.parameters.BackURL == 'PT' || $CurrentPage.parameters.BackURL == 'AP') && NOT(ApplicantInfo.Application_Stage__c == 'Working') && isPartner}" value="/apex/pr_app_list"><span>Back to Main Page</span></apex:outputLink>
	                        
                        	<!--
                        	<apex:outputLink styleClass="no-padding" rendered="{! NOT(Applicant_Info__c.Application_Level__c == 'Basic' && ApplicantInfo.Application_Stage__c == 'Working') && isPartner}" value="apex/applicantsInfo"><span>Back to Main Page</span></apex:outputLink>
                            <apex:outputLink styleClass="no-padding" rendered="{! NOT(Applicant_Info__c.Application_Level__c == 'Main') && applicantInfo.Type__c == 'Participant' && not(IsPartner)}" value="apex/your_applications?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
                            <apex:outputLink styleClass="no-padding" rendered="{! NOT(Applicant_Info__c.Application_Level__c == 'Main') && IsPartner }" value="apex/IGI_Preapp_Home?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
                            <apex:outputLink styleClass="no-padding" rendered="{! Applicant_Info__c.Application_Level__c == 'Main'}" value="apex/your_applications?id={!applicantInfo.Id}"><span>Back to Main Page</span></apex:outputLink>
                            -->
                        </div>
                   </div>
                   
                   <!-- Section Header -->
                   <div class="section-header clearfix">
                       <h2>Summary</h2>
                   </div> 
                   
                   <div class="white-box clearfix">
                              
                       <!-- Basic Instructions -->
                       <div class="panel no-float half">
                           <p>Please review your answers carefully before you submit your Basic Application. 
                               Click <strong>View PDF Summary</strong> to print or save your information for your records.</p>
                           <p>Once you have reviewed all of your information, click <strong>Submit Application</strong> to 
                               complete the Basic Application.</p>
                       </div>
              
                  	   <!-- Page Panel -->
                  	   <div>
          
	          			  <!-- Panel Heading -->
	                      <h3 class="data-block-title">Program Selection</h3> 
	                      
	                      <!-- What Program are you applying for? -->
	                      <div class="data-row">
	                          <div class="data-label left"> What program are you applying for? </div>
	                          <div class="data-field">
	                              <apex:outputField id="Intrax_Program_Options" value="{!applicantInfo.Intrax_Program_Options__c}" />
	                          </div>
	                      </div>
	                      </div>
	                      
	                      <!-- Page Panel -->
                  	   <div>
          
	          			  <!-- Panel Heading -->
	                      <h3 class="data-block-title">Program Preferences</h3> 
	                      
	                      <!-- Other Company -->
	                      <apex:outputPanel rendered="{! applicantInfo.Intrax_Program_Options__c == 'Business Internship' || applicantInfo.Intrax_Program_Options__c == 'Hospitality Internship' }">
	                      <div class="data-row">
	                          <div class="data-label left">Have you already secured an internship offer from a company?</div>
	                          <div class="data-field">
	                              <apex:outputText id="ServiceLevelYes" value="Yes" rendered="{!applicantInfo.Service_Level__c == 'Independent'}" />
	                              <apex:outputText id="ServiceLevelNo" value="No" rendered="{!applicantInfo.Service_Level__c == 'Premium'}" />
	                          </div>
	                      </div>
	                      </apex:outputPanel>
	                      
	                      <!-- Engagement Start -->
	                      <div class="data-row">
	                          <div class="data-label left"> Earliest Start Date </div>
	                          <div class="data-field">
	                              <apex:outputField id="Engagement_Start" value="{!applicantInfo.Engagement_Start__c}" />
	                          </div>
	                      </div>
	              
	                      <!-- Engagement End -->
	                      <div class="data-row">
	                          <div class="data-label left"> Latest End Date </div>
	                          <div class="data-field">
	                              <apex:outputField id="Engagement_End" value="{!applicantInfo.Engagement_End__c}" />
	                          </div>
	                      </div>
	              
	              		  <!-- Requested Duration -->	
	                      <div class="data-row">
	                          <div class="data-label left"> Requested Duration </div>
	                          <div class="data-field">
	                              <apex:outputField id="Program_Duration" value="{!applicantInfo.Program_Duration__c}" />
	                          </div> 
	                      </div>
	               
	               		  <!-- Do you have Flexible Dates? -->	
	                      <div class="data-row">
	                          <div class="data-label left">Do you have Flexible Dates?</div>
	                          <div class="data-field">
	                              <apex:outputField id="Flexible_Dates" value="{!applicantInfo.Flexible_Dates__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- What is your desired destination country? -->
	                      <div class="data-row">
	                          <div class="data-label left">What is your desired destination country?</div>
	                          <div class="data-field">
	                              <apex:outputField id="Country_of_Interest" value="{!applicantInfo.Country_of_Interest__c}"/>
	                          </div>
	                      </div>
	                      
	                      	                      
	                      <!-- Desired location notes -->
	                      <div class="data-row">
	                          <div class="data-label left">Desired location notes</div>
	                          <div class="data-field">
	                              <apex:outputField id="Location_of_Interest" value="{!applicantInfo.Location_of_Interest__c}"/>
	                          </div>
	                      </div>
	                      
	                      
	                      <apex:pageBlock rendered="{!applicantInfo.Intrax_Program_Options__c == 'ProWorld Internship'}" >
	                      <div class="data-row">
	                          <div class="data-label left">Select project(s) of interest</div>
	                          <div class="data-field">
	                              <apex:outputField id="Projects_of_Interest" value="{!applicantInfo.Projects_of_Interest__c}"/>
	                          </div>
	                      </div>
	                      </apex:pageBlock>
	                      
	                      <apex:pageBlock rendered="{!applicantInfo.Intrax_Program_Options__c == 'WEST' || applicantInfo.Intrax_Program_Options__c == 'Business Internship'}" >
	                      <div class="data-row">
	                          <div class="data-label left">Select Functional Area(s)</div>
	                          <div class="data-field">
	                              <apex:outputField id="Functional_Areas" value="{!applicantInfo.Functional_Areas__c}"/>
	                          </div>
	                      </div>
	                      </apex:pageBlock>
	                      
	                      <apex:pageBlock rendered="{!applicantInfo.Intrax_Program_Options__c == 'Hospitality Internship'}" >
	                      <div class="data-row">
	                          <div class="data-label left">Select Position Type(s)</div>
	                          <div class="data-field">
	                              <apex:outputField id="Position_Types" value="{!applicantInfo.Position_Types__c}"/>
	                          </div>
	                      </div>
	                      </apex:pageBlock>
	                      
	                       <!-- Location 1st Choice -->
	                      <apex:outputPanel rendered="{! applicantInfo.Intrax_Program_Options__c == 'WEST'}" >
	                      <div class="data-row">
	                          <div class="data-label left">Location 1st Choice</div>
	                          <div class="data-field">
	                              <apex:outputField id="Location_Preference_1" value="{!applicantInfo.Location_Preference_1__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Location 2nd Choice -->
	                      <div class="data-row">
	                          <div class="data-label left">Location 2nd Choice</div>
	                          <div class="data-field">
	                              <apex:outputField id="Location_Preference_2" value="{!applicantInfo.Location_Preference_2__c}"/>
	                          </div>
	                      </div>
	                      
	                      <!-- Location 3st Choice -->
	                      <div class="data-row">
	                          <div class="data-label left">Location 3st Choice</div>
	                          <div class="data-field">
	                              <apex:outputField id="Location_Preference_3" value="{!applicantInfo.Location_Preference_3__c}"/>
	                          </div>
	                      </div>
	                      </apex:outputPanel>
	                      
	                      <!-- Program Preferences Comments -->
	                      <div class="data-row">
	                          <div class="data-label left">Program Preferences Comments</div>
	                          <div class="data-field">
	                              <apex:outputField id="Program_Preference_Comment"  value="{!applicantInfo.Program_Preference_Comment__c}"/>
	                          </div>
	                      </div>
	                      </div>
	                      
	                      <!-- Page Panel -->
                  	   <div>
          
	          			  <!-- Panel Heading -->
	                      <h3 class="data-block-title">Biographical Details</h3> 
	                      
	                      <!-- Last Name -->
	                      <div class="data-row">
	                          <div class="data-label left"> First Name </div>
	                          <div class="data-field">
	                              <apex:outputField id="First_Name" value="{!personInfo.First_Name__c}" />
	                          </div>
	                      </div>
	                      
	                      <!-- First Name -->
	                      <div class="data-row">
	                          <div class="data-label left"> Middle Name </div>
	                          <div class="data-field">
	                              <apex:outputField id="Middle_Name" value="{!personInfo.Middle_Name__c}" />
	                          </div>
	                      </div>
	                         
