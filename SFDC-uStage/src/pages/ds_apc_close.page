<apex:page docType="html-5.0" applyBodyTag="false" applyHtmlTag="false"
	standardStylesheets="false" showHeader="false"  title="DocuSign Close Processing" id="ds_apc_close" controller="DocuSign_Controller" action="{!MethodAPCADEnv}">
<apex:composition template="intraxtemplate_vs3">
<apex:define name="body">
<style>
#branding-zone, #footer-zone, #menu-zone{display:none;}
#content-zone{background:transparent;}
</style>

<script>
j$(document).ready(function() {
setTimeout(function(){window.opener.location=window.opener.location.href;window.close();}, 12000);
//setTimeout('window.close();', 5000);
});
</script>



<div class="row">
<div id="sys-msg" class="col-md-6 col-lg-5">
<div id="sys-msg-inner">
<i class="fa fa-refresh fa-spin"></i>
<p class="lead">Your agreement is being processed.<br/> You will be re-directed to your portal shortly.</p>
</div>
</div><!--  end column -->
</div><!--  end row -->
</apex:define>

	</apex:composition>  
	

</apex:page>