                                                                <apex:outputText value="{!mseletionlst.se.Engagement__r.Interview_Availability__c}"/>
                                                            </div>
                                                            
                                                        </div><!-- end row -->
                                                        
                                                    </div><!-- end col -->
                                                </div><!-- end row -->
                                                <hr class="margin-b3"/>
                                            </apex:repeat>
                                        </div>
                                    </div>
                                    <!-- end col --> 
                                </div>
                                <!-- end row -->
                            </apex:pageBlock>
                            
                            <div class="sort-container vs1 margin-t4 form-vertical">
