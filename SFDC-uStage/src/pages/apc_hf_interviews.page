<apex:page id="apc_hf_interviews" docType="html-5.0" applyHtmlTag="false" applyBodyTag="false" action="{!CheckExpDate}" title="Your Interviews" standardStylesheets="false" showHeader="false" controller="apc_match_controller">
    <apex:composition template="intraxtemplate_vs3">
        <apex:define name="body">
            <style>
                .list-view.form-vertical .control-label {
                margin-bottom: 0;
                }
                .list-view.form-vertical .form-group {
                margin-bottom: 10px;
                }
                .list-view.vs1 .item .item-image {
                position: relative;
                width: 100%;
                background: #dcdcdc;
                text-align: center;
                }
                .item-image-male, .item-image-female {
                min-height: 160px;
                }
                .item-image-male, .item-image-female {
                min-height: 160px;
                }
                .list-view .item-image-real {
                width:100%;
                //max-height: 160px;
                }
                .phot-pre-img {
                /* display: block; */
                height: 160px;
                }
            </style>
            <apex:form id="webform">
                <apex:variable value="{!0}" var="addIndex" id="rowCounterF" />
                
                <!-- Jose B-02819: NON Requested or Confirmed status -->
                <apex:outputPanel layout="block" rendered="{!If(MatchConf.size == 0 || (MatchConf.size > 0 && MatchConf[0].Status__c !='Confirmed'),true,false)}">
                    
                    <h1 class="no-margin-t center-text">Your Interviews</h1>
                    
                    <!-- JOSE B-03139 Display Match Request Deadline messages in HF and PT portals (Match Deadlines - B) -->
                    <div class="row">
                    <div class="col-sm-10 center-block center-text">
                    <c:block_portal_msg_early_match APCArrivalDates="{!APCArrivalDates}"/>
                    </div>
                    </div>
                     <!-- END JOSE B-03139 -->
                     <!-- JOSE B-03139 Display Match Request Deadline messages in HF and PT portals (Match Deadlines - B) -->
                    <div class="row">
                    <div class="col-sm-10 center-block center-text">
                    <apex:outputPanel layout="block" rendered="{!MatchConfOld != null && MatchConfOld.SIZE > 0 && MatchConfOld[0].End_Date__c - 90 >= TOday()}">
				    <c:block_portal_msg_match_deadline PreviousMatch="{!MatchConfOld}"/>
				    </apex:outputPanel>
                    </div>
                    </div>
                     <!-- END JOSE B-03139 -->
                    
                    <apex:outputPanel layout="block" rendered="{!If(ActiveInterview.size > 0 && ConfMatch.size == 0,true,false)}">
                        <div class="row">
                            <div class="col-sm-10 center-block center-text">
                                <div class="alert alert-warning "><b class="sm">Interviewing au pairs? Call 800.428.7247 and dial 0 to get connected for FREE! Monday-Friday. 5am-5:30pm PST.</b></div>
                            </div>
                            <!-- end col --> 
                        </div>
                        <!-- end row -->
                    </apex:outputPanel>
                    
                    
                    <!-- Match Not found Panel-->
                    <apex:outputPanel layout="block" id="NoInterview" rendered="{!If(ActiveInterview.size==0 && InActivelst.size == 0 && ConfMatch.size==0,true,false)}">
                        <div class="row">
                            <div class="col-sm-10 center-block">
                                <div class="box gray lead center-text">You do not have any interviews yet
                                    <apex:outputLink id="Reqtmtch2" value="/apc_hf_search_au_pairs"> <b>Search au pairs ></b> </apex:outputLink>
                                </div>
                            </div>
                            <!-- end col --> 
                        </div>
                        <!-- end row -->
                    </apex:outputPanel>
                    <!-- End of Match Not found Panel--> 
                    
                    <!-- Current/Past Match Panel-->
                    <apex:outputPanel layout="block" id="viewInterview" rendered="{!ConfMatch.size == 0}">
                        
                        <apex:outputPanel layout="block" id="activeList" rendered="{!activeInterview.size > 0}">
                            <apex:pageBlock id="pb">
                                
                                <div class="row">
                                    <div class="col-sm-10 center-block">
                                        <h2 class="margin-b4">Current Interviews</h2> 
                                    </div>
                                </div><!-- end row --> 
                                
                                <div class="row">
                                    <div class="col-sm-10 center-block">
                                        <div class="list-view form-vertical">
                                            <apex:repeat id="repeatActiveInterview" value="{!activeInterview}" var="mseletionlst">
                                                
                                                <div class="row">
                                                    <div class="col-sm-3 center-text">
                                                        <apex:outputText rendered="{!mseletionlst.Engagement__c != null}">
                                                            <apex:outputLink value="/apex/apc_hf_au_pair_profile?id={!mseletionlst.Engagement__r.id}" id="linkprofinfo1"> 
                                                                <!--<apex:image styleClass="item-image-male" value="{!URLFOR($Resource.intrax_portal_images, '/intrax_portal_images/all/icons/male-profile-med.png')}"/>-->
                                                                <apex:image styleClass="phot-pre-img" value="{!returnURLmap[mseletionlst.Engagement__c]}" rendered="{!returnURLMap[mseletionlst.Engagement__c] != 'no-picture'}"/>
                                                                <apex:image styleClass="item-image-female" value="{!URLFOR($Resource.intrax_portal_images, '/intrax_portal_images/all/icons/female-profile-med.png')}"  rendered="{!returnURLMap[mseletionlst.Engagement__c] == 'no-picture' && (mseletionlst.Engagement__r.Gender__c == 'Female' || mseletionlst.Engagement__r.Gender__c == null )}"/>
                                                                <apex:image styleClass="item-image-male" value="{!URLFOR($Resource.intrax_portal_images, '/intrax_portal_images/all/icons/male-profile-med.png')}" rendered="{!returnURLMap[mseletionlst.Engagement__c] == 'no-picture' && mseletionlst.Engagement__r.Gender__c == 'Male'}"/>
                                                            </apex:outputLink>
                                                        </apex:outputText>
                                                        
                                                        <div>
                                                            <apex:commandlink styleClass="btn btn-primary btn-lg  margin-t2 margin-b1" id="Reqmatch" action="/apc_hf_match_request?id={!mseletionlst.id}&engid={!mseletionlst.Engagement__r.id}" 
                                                                              rendered="{!CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'045') &&
                                                                                        CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'009') && 
                                                                                        ApplicantInfoList[0].Terms_Accepted__c==true
                                                                                        && mseletionlst.Engagement__r.Placement_Status__c != 'Pending' && 
                                                                                        mseletionlst.Engagement__r.Placement_Status__c != 'Confirmed'}">
                                                                <apex:commandButton value="Request Match" id="Reqbtn"  />
                                                            </apex:commandlink>
                                                            
                                                            <apex:commandlink styleClass="btn btn-primary btn-lg  margin-t2 margin-b1" id="Reqmatch2" onComplete="rerunjs();callstepsmodal();" 
                                                                              rendered="{!(Not(CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'045')) || 
                                                                                        Not(CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'009')) || 
                                                                                        ApplicantInfoList[0].Terms_Accepted__c==False) && ApplicantInfoList[0].Application_Type__c == 'Original'
                                                                                        && mseletionlst.Engagement__r.Placement_Status__c != 'Pending' && 
                                                                                        mseletionlst.Engagement__r.Placement_Status__c != 'Confirmed'}">
                                                                <apex:commandButton value="Request Match" id="Incomp" />
                                                                <apex:param Name="requestId" value="{!mseletionlst.id}" assignTo="{!requestId}"/>
                                                                <apex:param Name="enggId" value="{!mseletionlst.Engagement__r.id}" assignTo="{!SelcEngIds}"/>
                                                            </apex:commandlink>
                                                            
                                                            <!-- Request Button For Renewal-Rematch -->
                                                            <apex:commandlink styleClass="btn btn-primary btn-lg  margin-t2 margin-b1" id="Reqmatchren" action="/apc_hf_match_request?id={!mseletionlst.id}&engid={!mseletionlst.Engagement__r.id}" 
                                                                              rendered="{!ApplicantInfoList[0].Application_Type__c != 'Original'}">
                                                                <apex:commandButton value="Request Match" id="ReqbtnRen"  />
                                                            </apex:commandlink>
                                                            
                                                        </div>
                                                        
                                                        <div>
                                                            <apex:commandlink id="cancel" action="{!checkInterviews}" onComplete="rerunjs();callmodal();" reRender="webform" >
                                                                <apex:commandButton value="Cancel Interview" id="continueButton"/>
                                                                <apex:param Name="requestId" value="{!mseletionlst.id}" assignTo="{!requestid}"/>
                                                            </apex:commandlink>
                                                        </div>
                                                        
                                                    </div>
                                                    <!--  end col -->
                                                    
                                                    <div class="col-sm-9">
                                                        <h3 class="no-margin-t">{!mseletionlst.Engagement__r.Account_Id__r.Name}</h3>
                                                        <div class="row">
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Skype</label>
                                                                    <apex:outputText value="{!mseletionlst.Engagement__r.Account_Id__r.Skype_ID__pc}"/>
                                                                </div>
                                                            </div>
                                                            
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Email</label>
                                                                    <apex:outputText value="{!mseletionlst.Engagement__r.Account_Id__r.PersonEmail}"/>
                                                                </div>
                                                            </div>
                                                           <!--  <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Phone Country Code</label>
                                                                    <apex:outputText value="{!SelectionCountryCode[mseletionlst.Id]}"/> 
                                                                </div>
                                                            </div> -->
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Phone</label>
                                                                   <apex:outputText value="{!SelectionCountryCode[mseletionlst.Id] + ' ' + mseletionlst.Engagement__r.Account_Id__r.PersonHomePhone}"/> 
                                                                 
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end row -->
                                                        
                                                        <div class="row">
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">City</label>
                                                                    <apex:outputText value="{!mseletionlst.Engagement__r.Account_Id__r.BillingCity}"/>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Country</label>
                                                                    <apex:outputText value="{!mseletionlst.Engagement__r.Account_Id__r.BillingCountry}"/>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Current Time for Au Pair</label>
                                                                    
                                                                    <!--  <apex:outputText value="{0,date,yyyy.MM.dd G 'at' HH:mm:ss z}"> -->
                                                                    <apex:outputText value="{0,date, MMMM d, yyyy h:mm a}">
                                                                        <apex:param value="{!ptTimeZone[mseletionlst.Engagement__r.Account_Id__r.PersonEmail]}" />
                                                                        <!-- <apex:param value="{!NOW()+offset}" />-->
                                                                    </apex:outputText> 
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end row -->
                                                        
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label class="control-label">Best Time to Contact</label>
                                                                <apex:outputText value="{!mseletionlst.Engagement__r.Interview_Availability__c}"/>
                                                                <br/>
                                                            </div>
                                                            <!-- end col --> 
                                                        </div>
                                                        <!-- end row -->
                                                        
                                                        <div class="row margin-t2">
                                                            <div class="col-sm-12">
                                                                <!--<label class="control-label">You have {!hoursnew[mseletionlst.id]} hours left for this interview</label>-->
                                                                <label>Interview Expires On
                                                                    <apex:outputText value="{0,date, MMMM d, yyyy h:mm a}" >
                                                                        <apex:param value="{!mseletionlst.Interview_End_Date__c+offset}"/>
                                                                    </apex:outputText>
                                                                </label>
                                                            </div>
                                                            <!-- end col --> 
                                                        </div>
                                                        <!-- end row --> 
                                                    </div>
                                                    <!-- end col --> 
                                                </div>
                                                <!-- end row -->
                                                <hr class="margin-b3 "/>
                                                <apex:variable var="addIndex" value="{!addIndex + 1}" />
                                            </apex:repeat>
                                        </div>
                                    </div>
                                    <!-- end col --> 
                                </div>
                                <!-- end row -->
                            </apex:pageBlock>
                        </apex:outputPanel>
                        
                        <apex:outputPanel layout="block" id="inactiveListpanel" rendered="{!PastSelections.size > 0}">
                            <apex:pageBlock id="pastInterviewblk">
                                <div class="row">
                                    <div class="col-sm-10 center-block">
                                        <div class="list-view form-vertical">
                                            <h2 class="margin-b4">Past Interviews</h2>
                                            <apex:repeat id="repeatInActiveInterview" value="{!PastSelections}" var="mseletionlst">
