                                                                     <apex:outputText value="{0,date, MMMM d, yyyy h:mm a}">
                                                                        <apex:param value="{!ptTimeZone[mseletionlst.se.Engagement__r.Account_Id__r.PersonEmail]}" />
                                                                         <!--<apex:param value="{!NOW()+offset}" /> -->
                                                                    </apex:outputText>
                                                                </div>
                                                            </div>  
                                                        </div><!-- end row -->
                                                        
                                                        <div class="row"> 
