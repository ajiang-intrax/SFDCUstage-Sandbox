                                                                              rendered="{!CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'045') && CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'009') && 
                                                                                        ApplicantInfoList[0].Terms_Accepted__c==true && mseletionlst.se.Engagement__r.Placement_Status__c != 'Pending' && 
                                                                                        mseletionlst.se.Engagement__r.Placement_Status__c != 'Confirmed'}">
                                                                <apex:commandButton value="Request Match" id="Reqbtn"  />
                                                            </apex:commandlink>
                                                            
                                                            <apex:commandlink styleClass="btn btn-primary btn-lg  margin-t2 margin-b1" id="PastReqmatch2" onComplete="rerunjs();callstepsmodal();" rendered="{!(Not(CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'045')) || 
                                                                                                                                                                                                             Not(CONTAINS(ApplicantInfoList[0].PageStatus_Complete__c,'009')) || 
