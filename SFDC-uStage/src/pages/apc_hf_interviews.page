                                                                <div class="form-group">
                                                                    <label class="control-label">Skype</label>
                                                                    <apex:outputText value="{!mseletionlst.se.Engagement__r.Account_Id__r.Skype_ID__pc}"/></div>
                                                            </div>
                                                            
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Email</label>
                                                                    <apex:outputText value="{!mseletionlst.se.Engagement__r.Account_Id__r.PersonEmail}"/>
                                                                </div>
                                                            </div>
                                                            
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Phone</label>
                                                                    <apex:outputText value="{!SelectionCountryCode[mseletionlst.se.Id] + ' ' + mseletionlst.se.Engagement__r.Account_Id__r.PersonHomePhone}"/></div>
                                                            </div>
                                                        </div><!-- end row -->
                                                        
                                                        <div class="row">
                                                            
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">City</label>
                                                                    <apex:outputText value="{!mseletionlst.se.Engagement__r.Account_Id__r.billingCity}"/>
                                                                </div>
                                                            </div>
                                                            
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Country</label>
                                                                    <apex:outputText value="{!mseletionlst.se.Engagement__r.Account_Id__r.billingCountry}"/>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Current Time for Au Pair</label>
